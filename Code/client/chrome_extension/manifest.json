{
    "name": "Malicious URL Detection",
    "description": "Benign or Malicious | Final Year Project",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "<all_urls>",
        "tabs",
        "storage"

    ],
    "background": {
        "scripts": ["assets/js/background.js", "assets/js/jquery-3.6.0.min.js"]
    },
    "web_accessible_resources": [
        "malicious_page.html"
    ],
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}